{
  "name": "brainhub-frontend",
  "version": "0.1.0",
  "private": true,
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/parser": "^5.42.1",
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.3",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.8",
    "sass": "^1.55.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.3",
    "socket.io-client": "^4.5.3",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:unit": "react-scripts test  --transformIgnorePatterns node_modules/(?!@codemirror)/ --watchAll=false",
    "test:e2e": "wait-on http://localhost:3000 && npm run check-types:cypress && npm run cypress",
    "test:e2e:interactive": "wait-on http://localhost:3000 && npm run check-types:cypress && cypress open --project src/cypress-tests",
    "check-types:cypress": "tsc -p src/cypress-tests/cypress/tsconfig.json",
    "cypress": "cypress run --project src/cypress-tests --spec \"**/*.feature\" --browser chrome",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^14.0.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/react": "^7.0.1",
    "@cypress/webpack-dev-server": "^3.1.0",
    "cypress": "^9.1.0",
    "cypress-cucumber-preprocessor": "^4.3.0",
    "eslint": "^8.27.0",
    "react-test-renderer": "^18.2.0",
    "resolve": "^1.20.0",
    "wait-on": "^6.0.1"
  }
}
